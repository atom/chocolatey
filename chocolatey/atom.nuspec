<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Atom</id>
    <title>Atom</title>
    <version>0.174.0</version>
    <authors>https://github.com/orgs/atom/members</authors>
    <owners>Brad Gearon</owners>
    <summary>Atom: A hackable text editor for the 21st Century</summary>
    <description>
      Read more on http://atom.io

      This package always installs the latest version of Atom available.
      So the version of Atom installed may be later than the version listed
      as the chocolatey package version.

      The version of Atom installed will also automatically update so you won't
      need to upgrade it manually via chocolatey.
    </description>
    <projectUrl>http://atom.io</projectUrl>
    <tags>atom notepad text github</tags>
    <copyright/>
    <licenseUrl>https://github.com/atom/atom/LICENSE.md</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://raw.githubusercontent.com/atom/atom/master/resources/atom.png</iconUrl>
    <releaseNotes>https://atom.io/releases</releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools"/>
  </files>
</package>
